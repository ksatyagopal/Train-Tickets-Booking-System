@model BookMyTrainAdminClientApp.Models.Fare

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Fare</h1>
<div class="text-right">
    <a asp-action="Index">Back to List</a>
</div>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TypeOfCoach" class="control-label"></label>
                <input asp-for="TypeOfCoach" class="form-control" />
                <span asp-validation-for="TypeOfCoach" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fare1" class="control-label"></label>
                <input asp-for="Fare1" class="form-control" />
                <span asp-validation-for="Fare1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
